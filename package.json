{
  "name": "popthecorn",
  "version": "0.1.0",
  "description": "popthecorn",
  "license": "MIT",
  "engines": {
    "node": "4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/dbk-changelog"
    }
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "deploy": "npm run build && surge dist https://popthecorn.surge.sh",
    "clean": "rimraf dist/ coverage/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --progress --no-info",
    "lint": "npm run lint-js && npm run lint-css",
    "lint-css": "stylelint './src/**/*.css'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore manifest.json",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production",
    "pretest": "npm run clean coverage/ && npm run lint",
    "test": "karma start --singleRun true --browsers PhantomJS",
    "posttest": "istanbul check-coverage",
    "test:watch": "karma start --singleRun false --browsers Chrome",
    "report-coverage": "cat coverage/lcov.info | codecov",
    "validate": "npm ls"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-register": "^6.14.0",
    "dbk-changelog": "0.0.4",
    "enzyme": "^2.4.1",
    "eslint": "3.1.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-react": "^5.2.2",
    "gh-pages": "^0.11.0",
    "istanbul": "^1.0.0-alpha.2",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.1.0",
    "karma-sinon": "^1.0.5",
    "karma-source-map-support": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "react-addons-test-utils": "15.2.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "surge": "^0.18.0",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.11.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.9.0",
    "basscss": "^8.0.1",
    "basscss-addons": "^1.0.0-beta4",
    "basscss-align": "^1.0.2",
